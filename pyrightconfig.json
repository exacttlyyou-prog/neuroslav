{
  "include": [
    "apps/api/app"
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/.*",
    "_legacy_archive_v1",
    "apps/web",
    "apps/extractor"
  ],
  "ignore": [
    "**"
  ],
  "executionEnvironments": [
    {
      "root": "apps/api",
      "venvPath": ".",
      "venv": ".venv",
      "extraPaths": [
        "app"
      ]
    }
  ],
  "pythonVersion": "3.11",
  "pythonPlatform": "Darwin",
  "typeCheckingMode": "basic",
  "reportMissingImports": "warning"
}
