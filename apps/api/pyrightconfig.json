{
  "include": [
    "app"
  ],
  "exclude": [
    "**/__pycache__",
    "**/node_modules",
    ".venv",
    "**/.*",
    "../_legacy_archive_v1/**",
    "../../_legacy_archive_v1/**",
    "**/_legacy_archive_v1/**"
  ],
  "pythonVersion": "3.13",
  "pythonPlatform": "Darwin",
  "typeCheckingMode": "basic",
  "reportMissingImports": "warning",
  "reportMissingTypeStubs": false,
  "pythonPath": ".venv/bin/python,
  "extraPaths": [
    ".",
    "app"
  ],
  "venvPath": "..",
  "venv": ".venv",
  "executionEnvironments": [
    {
      "root": ".",
      "pythonVersion": "3.11",
      "pythonPlatform": "Darwin",
      "extraPaths": [
        ".",
        "app"
      ]
    }
  ]
}
